@model Modelo.Discente.Academico

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_LayoutIES.cshtml";
}

<h3>Você tem certeza que deseja apagar o acadêmico?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroAcademico)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RegistroAcademico)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nascimento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nascimento)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="AcademicoID" />
        <a asp-action="Index">Voltar para a lista</a>
        <input type="submit" value="Delete" class="btn btn-danger" /> 
    </form>
</div>
